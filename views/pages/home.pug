extends ../base.pug

block variables
    - var template = 'home'

block content
  .homepage
    .homepage__wrapper
      each section in homepage.data.body
        if section.slice_type == 'hero'
          section.homepage__hero
            .homepage__hero__wrapper
              .homepage__hero__cta
                h1.homepage__hero__title(data-animation="title")=section.primary.title
                a.cta__link(href='/quoter')
                  button.homepage__hero__button.button__primary Cotizar

              .homepage__hero__gallery
                each media in section.items
                  figure.homepage__hero__gallery__media
                    img.homepage__hero__gallery__media__image(alt=media.image.alt data-src=media.image.url)

        if section.slice_type == 'history'
          section.homepage__history(id='about')
            .homepage__history__wrapper
              .homepage__history__story
                h3.homepage__history__title(data-animation="paragraph")=section.primary.title
                .homepage__history__copy!=PrismicH.asHTML(section.primary.copy).replace(/<p>/g, '<p data-animation="paragraph">')

              .homepage__history__gallery
                each media in section.items
                  figure.homepage__history__gallery__media
                    img.homepage__history__gallery__media__image(alt=media.image.alt data-src=media.image.url)

        if section.slice_type == 'services'
          section.homepage__services(id='services')
            .homepage__services__wrapper
              h3.homepage__services__subtitle=section.primary.title
              .homepage__services__list
                each service in section.items
                  a.service__item(href="/quoter" data-image="img/1.jpg" data-animation="menu")
                    span.service__item__text
                      span.service__item__textinner=service.name
                    span.service__item__sub Cotizar

        if section.slice_type == 'callout'
          section.homepage__callout
            .homepage__callout__wrapper(style=`background-image: url(${section.primary.image.url})`)
              .homepage__callout__cta
                h1.homepage__callout__title(data-animation="title")=section.primary.title
                a.cta__link(href='/quoter')
                  button.homepage__callout__button.button__primary Cotizar

        if section.slice_type == 'contact'
          section.homepage__contact(id='contact')
            .homepage__contact__wrapper
              .homepage__contact__info
                a.homepage__contact__office(href=section.primary.google_maps_page)
                  h3.homepage__contact__office__title=section.primary.address_title
                  .homepage__contact__office__address
